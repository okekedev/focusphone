# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/focusphone

# Microsoft Entra ID (Azure AD)
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_ID=your-client-id

# MDM Server
MDM_SERVER_URL=https://your-mdm-server.azurecontainerapps.io
MDM_ORG_NAME=FocusPhone
MDM_ORG_IDENTIFIER=com.focusphone
MDM_TOPIC=com.apple.mgmt.External.your-push-topic

# Apple Push Notifications
APNS_KEY_ID=your-key-id
APNS_TEAM_ID=your-team-id
APNS_KEY_PATH=/app/certs/AuthKey.p8
APNS_PRODUCTION=true
